<div class="ui secondary pointing menu">
  <% if current_user&.admin? %>
    <%= link_to 'Home', admin_users_path, class: 'active item'%>
  <% else %>
    <%= link_to 'Home', dashboard_path, class: 'active item'%>
  <% end %>
  <div class="right menu">
    <% if user_signed_in? %>
      <%= link_to 'Logout', destroy_user_session_path, method: :delete , class: 'ui item' %>
    <% else %>
       <%= link_to 'Sign in', new_user_session_path, class: 'ui item' %>
    <% end %>
  </div>
</div>